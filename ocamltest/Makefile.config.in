# The configuration Makefile for ocamltest

ifeq "$(filter $(UNIXLIB),$(OTHERLIBRARIES))" ""
  unix_impl := dummy
  unix_name :=
  unix_path :=
  unix_include :=
else
  unix_impl := real
  unix_name := unix
  unix_path := $(ROOTDIR)/otherlibs/$(UNIXLIB)
  unix_include := -I $(unix_path) $(EMPTY)
  ifeq "$(UNIX_OR_WIN32)" "win32"
  else
  endif
endif
